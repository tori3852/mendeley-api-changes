    {
      "operations": [
        {
          "authorizations": {},
          "items": {
            "$ref": "UserDocument"
          },
          "method": "GET",
          "nickname": "search",
          "notes": "Multiple search fields may be specified. Callers must provide either a query, or at least one of title, author, source or abstract. Setting a minimum or maximum year excludes documents with no defined year.",
          "parameters": [
            {
              "allowMultiple": false,
              "description": "Query, matches any field",
              "name": "query",
              "paramType": "query",
              "required": false,
              "type": "string"
            },
            {
              "allowMultiple": false,
              "description": "Title",
              "name": "title",
              "paramType": "query",
              "required": false,
              "type": "string"
            },
            {
              "allowMultiple": false,
              "description": "Author",
              "name": "author",
              "paramType": "query",
              "required": false,
              "type": "string"
            },
            {
              "allowMultiple": false,
              "description": "Tag",
              "name": "tag",
              "paramType": "query",
              "required": false,
              "type": "string"
            },
            {
              "allowMultiple": false,
              "description": "Source, e.g. publication",
              "name": "source",
              "paramType": "query",
              "required": false,
              "type": "string"
            },
            {
              "allowMultiple": false,
              "description": "Abstract",
              "name": "abstract",
              "paramType": "query",
              "required": false,
              "type": "string"
            },
            {
              "allowMultiple": false,
              "description": "Minimum year",
              "format": "int32",
              "name": "min_year",
              "paramType": "query",
              "required": false,
              "type": "integer"
            },
            {
              "allowMultiple": false,
              "description": "Maximum year",
              "format": "int32",
              "name": "max_year",
              "paramType": "query",
              "required": false,
              "type": "integer"
            },
            {
              "allowMultiple": false,
              "description": "View",
              "enum": [
                "bib",
                "patent",
                "client",
                "tags",
                "all"
              ],
              "name": "view",
              "paramType": "query",
              "required": false,
              "type": "string"
            },
            {
              "allowMultiple": false,
              "description": "Use AND operator for search",
              "name": "use_and",
              "paramType": "query",
              "required": false,
              "type": "boolean"
            },
            {
              "allowMultiple": false,
              "description": "The maximum number of items on the page. If not supplied, the default is 10. The largest allowable value is 100.",
              "maximum": "100",
              "minimum": "1",
              "name": "limit",
              "paramType": "query",
              "required": false,
              "type": "string"
            }
          ],
          "produces": [
            "application/vnd.mendeley-document.1+json"
          ],
          "responseMessages": [
            {
              "code": 200,
              "message": "Success. Returns documents found if any."
            },
            {
              "code": 400,
              "message": "Invalid parameters supplied"
            }
          ],
          "summary": "<b>V1</b> - Search documents in library",
          "type": "array"
        }
      ],
